import numpy as np

units = np.array([[120, 140, 130],
                  [150, 160, 155],
                  [100, 110, 105]])

print("Electricity Usage:")
print(units)

# Double the 3rd column (Subsidy Month)
units[:, 2] = units[:, 2] * 2

print("\nAfter Subsidy:")
print(units)

print("\nIn Single Row Format:")
print(units.flatten())
